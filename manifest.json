{
    "name": "Tamatar",
    "description": "Pomodoro Timer",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": [
        "notifications"
    ],
    "app": {
        "background": {
            "scripts": ["src/scripts/background.js"]
        }
    },
    "icons": {
        "16": "src/assets/images/tamatar-16.png",
        "128": "src/assets/images/tamatar-notification.png"
    }
}